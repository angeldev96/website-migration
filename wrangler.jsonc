{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "nextjs",
    "main": ".open-next/worker.js",
    "compatibility_date": "2025-03-01",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    "assets": {
        "binding": "ASSETS",
        "directory": ".open-next/assets"
    },
    "observability": {
        "enabled": true
    },
    // Performance limits - adjust based on your plan
    "limits": {
        "cpu_ms": 50
    }
}
